{
    "name": "sllh/iso-codes-validator",
    "type": "library",
    "description": "Symfony validator wrapper of ronanguilloux/isocodes",
    "keywords": ["ISO codes", "Symfony", "Validator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan Senechal",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/validator": "~2.7",
        "symfony/translation": "~2.7",
        "ronanguilloux/isocodes": "^1.1.4"
    },
    "require-dev": {
        "satooshi/php-coveralls": "~0.6",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "suggest": {
        "sllh/iso-codes-validator-bundle": "For Symfony integration with ease",
        "sllh/iso-codes-validator-service-provider": "For Silex integration with ease"
    },
    "autoload": {
        "psr-4": { "SLLH\\IsoCodesValidator\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\IsoCodesValidator\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
